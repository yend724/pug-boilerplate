{
  "name": "template-html",
  "version": "1.0.0",
  "description": "Template for HTML development.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "run-s build:**",
    "build:clean": "rm -rf dist",
    "build:webpack": "webpack --config webpack.prd.js",
    "build:sass": "sass --style=compressed --no-source-map src:dist",
    "build:apf": "postcss dist/assets/css/index.css -u autoprefixer -o dist/assets/css/index.css",
    "build:img": "cpx \"src/assets/img/**/*.{jpg,png,gif,svg}\" dist/assets/img/",
    "start": "npm run watch",
    "watch": "run-p watch:**",
    "watch:webpack": "webpack --config webpack.dev.js -w",
    "watch:sass": "sass --embed-source-map src:dist -w",
    "watch:img": "cpx \"src/assets/img/**/*.{jpg,png,gif,svg}\" dist/assets/img/ -w",
    "watch:bs": "browser-sync start --config bs-config.js"
  },
  "keywords": [],
  "author": "yend724",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "browser-sync": "^2.27.9",
    "cpx": "^1.5.0",
    "fork-ts-checker-webpack-plugin": "^7.2.6",
    "glob": "^8.0.1",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "pug": "^3.0.2",
    "pug-loader": "^2.4.0",
    "sass": "^1.50.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "ie 11",
    "not dead"
  ],
  "dependencies": {}
}
